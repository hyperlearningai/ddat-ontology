        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Network Architecture</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A network architect is responsible for network designs and specifications, including cloud networks.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#leadDevOpsEngineer -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#leadDevOpsEngineer">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#devOpsEngineering"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#devOpsEngineering"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Act as a technical product owner, developing medium to long term strategies for product lines. 
2. Provide technical leadership and guidance through coaching and mentoring. 
3. Lead the sharing of knowledge and good practice.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">G7, G6</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/development-operations-devops-engineer.html#lead-devops-engineer"/>
        <rdfs:label xml:lang="en">Lead DevOps Engineer</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A lead DevOps engineer leads one or a small number of related project teams and contributes to the development of the strategic direction. There are 2 different roles at this level - a technical role and a management role.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#seniorProductManager -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#seniorProductManager">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#productManagement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#productManagement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Line manage associate product managers and product managers. 
2. Have expert product management skills. 
3. Potentially be involved in elements of programme management activity. 
4. Be involved in hiring associates, product managers and contractors.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">G7</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/product-manager.html#senior-product-manager"/>
        <rdfs:label xml:lang="en">Senior Product Manager</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Senior product managers manage multiple products or a single product that is especially complex, high risk or sensitive.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#juniorContentDesigner -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#juniorContentDesigner">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#contentDesign"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#contentDesign"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Explain content decisions. 
2. Work collaboratively. 
3. Work independently after being given direction by more senior content designers. 
4. Independently identify user issues and needs. 
5. Support content team administration and publishing processes. 
6. Engage with and contribute to the cross-government content community.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EO, HEO</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/content-designer.html#junior-content-designer"/>
        <rdfs:label xml:lang="en">Junior Content Designer</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A junior content designer is a graduate with a degree in a relevant subject or an individual with some relevant work experience in content creation.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#seniorDeliveryManager -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#seniorDeliveryManager">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#deliveryManagement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#deliveryManagement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Have strong communication skills and engage senior stakeholders. 
2. Coach and mentor delivery managers</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SEO, G7</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/delivery-manager.html#senior-delivery-manager"/>
        <rdfs:label xml:lang="en">Senior Delivery Manager</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A senior delivery manager is accountable for the effective delivery of complex, high risk products and services. The role is similar to a delivery manager role, but senior delivery managers have more experience across a range of products and services, throughout the entire product life cycle, and have greater responsibility and accountability as the main point of escalation.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#headOfFrontendDevelopment -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#headOfFrontendDevelopment">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#frontendDevelopment"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#frontendDevelopment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Define and assure best practice while influencing, leading and mentoring others. 
2. Influence and develop organisational strategy and priorities, collaborating with counterpart colleagues across government. 
3. Ensure the right conditions and environment for frontend developers to work effectively. 
4. Develop departmental capability by facilitating internal mobility, shaping career paths and recruiting talent.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">G6</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/frontend-developer.html#head-of-frontend-development"/>
        <rdfs:label xml:lang="en">Head of Frontend Development</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A head of frontend development is an expert practitioner with broad industry experience. They are an expert in many technical areas, or specialists with very deep knowledge in a particular technical area.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#leadProductManager -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#leadProductManager">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#productManagement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#productManagement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Be as interested in managing people as managing products. 
2. Work closely with the head of product (where they exist as a separate role) to provide leadership and direction to all products in a programme or a local product community. 
3. Maintain the high level roadmap. 
4. Be involved in a range of programme management activities in support of the service owner or programme lead. 
5. Be involved in hiring associate product managers, product managers, senior product managers and contractors. 
6. Provide leadership in your programmes and across the local community.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">G7, G6</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/product-manager.html#lead-product-manager"/>
        <rdfs:label xml:lang="en">Lead Product Manager</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A lead product manager is the lead for the product team in their programme or service.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#leadDeveloperManager -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#leadDeveloperManager">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#softwareDevelopment"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#softwareDevelopment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Guide the ways in which the team works. 
2. Be proficient in a wide range of technical systems. 
3. Be involved in identifying appropriate technology and approaches, deciding when software should be written. 
4. Contribute to the developer community. 
5. Provide technical leadership, coaching and mentoring your team. 
6. Promote knowledge sharing and adoption of good practice.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">G7, G6</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/software-developer.html#lead-developer---management"/>
        <rdfs:label xml:lang="en">Lead Developer Manager</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A lead developer leads development on one or a small number of related project teams. There are 2 different roles at this level - a technical role and a management role.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#changeAndReleaseManager -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#changeAndReleaseManager">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#changeAndReleaseManagement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#changeAndReleaseManagement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Provide an escalation point. 
2. Make decisions for all technical changes. 
3. Coordinate releases and interdependencies.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HEO, SEO</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/change-and-release-manager.html#change-and-release-manager"/>
        <rdfs:label xml:lang="en">Change and Release Manager</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A change and release manager ensures that technical changes to IT services are adequately assessed for impact, prioritised, scheduled, authorised and implemented. They ensure this is done in line with processes and using appropriate tools. They chair the Change Advisory Board (CAB).</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#headOfAgileDeliveryManagement -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#headOfAgileDeliveryManagement">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#deliveryManagement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#deliveryManagement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Represent and champion the role within your department, across government and in industry. 
2. Lead the community of practice for this role and build capability and excellence (in an Agile, Lean practice). 
3. Be responsible for the recruitment of the right people to the right teams. 
4. Support professional development and continuous improvement of your community. 
5. Work with other heads of roles to promote effective cross-functional delivery. 
6. Be a skilled team leader who can confidently communicate the value of the role to digital and non-digital stakeholders. 
7. Be credible and influential across departments.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">G6</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/delivery-manager.html#head-of-agile-delivery-management"/>
        <rdfs:label xml:lang="en">Head of Agile Delivery Management</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A head of (Agile) delivery management is an experienced practitioner who exemplifies what good looks like across the delivery roles.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#dataArchitect -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#dataArchitect">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#dataArchitecture"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#dataArchitecture"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Design, support and provide guidance for the upgrade, management, decommission and archive of data in compliance with data policy. 
2. Provide input into data dictionaries. 
3. Define and maintain the data technology architecture, including metadata, integration and business intelligence or data warehouse architecture.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HEO, SEO</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/data-architect.html#data-architect"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data Architect</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A data architect designs and builds data models to fulfil the strategic data needs of the organisation, as defined by chief data architects.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#headOfGraphicDesign -->

    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#headOfGraphicDesign">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#graphicDesign"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#specialistIn"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#graphicDesign"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Influence both design and organisational strategy and priorities. 
2. Collaborate with counterpart colleagues across government. 
3. Focus on ensuring the right conditions and environment for designers to work effectively.</responsibilities>
        <civilServiceJobGrades xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">G6</civilServiceJobGrades>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</entityType>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/graphic-designer.html#head-of-graphic-design"/>
        <rdfs:label xml:lang="en">Head of Graphic Design</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A head of graphic design is an expert practitioner with broad industry experience, who can define and assure best practice while influencing, leading and mentoring others.</skos:definition>
    </owl:Class>



