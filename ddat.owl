        <rdfs:label xml:lang="en">Trainee Data Scientist</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A trainee data scientist is inquisitive, curious about data and keen to learn and develop.</skos:definition>
    </owl:Class>
    


