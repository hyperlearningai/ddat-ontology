        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Network Architect</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A network architect has responsibility for network designs and specifications to support business strategies using common tools.</skos:definition>
    </owl:Class>
    


    <!-- http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#leadTestEngineer -->

