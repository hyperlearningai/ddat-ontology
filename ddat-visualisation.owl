        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A software developer designs, runs and improves software that meets user needs.</skos:definition>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Responsible for writing clean, secure code following a test-driven approach. 
2. Create code that is open by default and easy for others to reuse.</responsibilities>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/software-developer.html"/>
    </owl:Class>


    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#accessibility">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#userCenteredDesign"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#branchOf"/>
