        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#branchOf"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#userCenteredDesign"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Branch</entityType>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Content Strategy</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A content strategist takes an overview of content and how it is produced.</skos:definition>
        <responsibilities xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1. Design governance, workflows and taxonomies in a user-centred context. 
2. Work with senior stakeholders, user researchers and content, service and interaction designers.</responsibilities>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/content-strategist.html"/>
    </owl:Class>


    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#graphicDesign">
