        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#branchOf"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#userCenteredDesign"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Branch</entityType>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Graphic Design</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A graphic designer creates graphic elements that underpin interaction and service design. You will use layout, spacing, colour, type and iconography to ensure that content is legible and readable and that users see and understand interactions.</skos:definition>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/graphic-designer.html"/>
    </owl:Class>


    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#interactionDesign">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#userCenteredDesign"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#branchOf"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#userCenteredDesign"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <entityType xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Branch</entityType>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Interaction Design</rdfs:label>
        <skos:definition xml:lang="en" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An interaction designer works out the best way to let users interact with services, in terms of both overall flow and at the level of individual design elements.</skos:definition>
        <url xml:lang="en" rdf:resource="https://ddat-capability-framework.service.gov.uk/interaction-designer.html"/>
    </owl:Class>


    <owl:Class rdf:about="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#serviceDesign">
        <rdfs:subClassOf rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#userCenteredDesign"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#branchOf"/>
                <owl:someValuesFrom rdf:resource="http://daas.hyperlearning.ai/architecture/data/ontologies/ddat#userCenteredDesign"/>
